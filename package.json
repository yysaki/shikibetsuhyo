{
  "name": "shikibetsuhyo",
  "version": "0.1.0",
  "license": "MIT",
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "jquery-mobile": "./node_modules/jquery-mobile-babel-safe/js/jquery.mobile-1.4.5.js"
  },
  "engines": {
    "node": ">=6.9.1",
    "npm": ">=3.10.8"
  },
  "style": [
    "less/style.less",
    "node_modules/jquery-mobile-babel-safe/css/jquery.mobile-1.4.5.min.css"
  ],
  "browserify-shim": {
    "jquery": "$",
    "jquery-mobile": {
      "depends": "jquery:$"
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-preset-es2015": "^6.16.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "browserify-shim": "^3.8.12",
    "chai": "^4.1.2",
    "clean-css-cli": "^4.1.11",
    "express": "^4.14.0",
    "jquery": "^2.2.4",
    "jquery-mobile-babel-safe": "^1.4.5-1",
    "less-css-stream": "^1.0.0",
    "mocha": "^5.2.0",
    "parcelify": "^2.2.0",
    "pug": "^2.0.0-beta6",
    "supertest": "^3.1.0",
    "uglify-js": "^3.4.0",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "build": "babel es6 -d js && browserify js/main.js -o public/js/bundle.js -p [ parcelify -o public/css/bundle.css -t less-css-stream -d . ] && uglifyjs public/js/bundle.js -c -o public/js/bundle.min.js --source-map && cleancss public/css/bundle.css -o public/css/bundle.min.css",
    "postinstall": "npm run build",
    "start": "node js/app.js",
    "test": "babel es6 -d js && babel test-es6 -d test-js && mocha test-js/**/*.js",
    "watch": "watchify -t babelify es6/main.js -o public/js/bundle.js -p [ parcelify -wo public/css/bundle.css -t less-css-stream -d . ] --debug --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yysaki/shikibetsuhyo.git"
  },
  "bugs": {
    "url": "https://github.com/yysaki/shikibetsuhyo/issues"
  },
  "homepage": "https://github.com/yysaki/shikibetsuhyo"
}
