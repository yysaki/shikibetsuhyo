{
  "name": "shikibetsuhyo",
  "version": "0.1.0",
  "browser": {
    "jquery": "./bower_components/jquery/dist/jquery.js"
  },
  "engines": {
    "node": "0.12.0",
    "npm": "2.5.1"
  },
  "style": [
    "less/style.less"
  ],
  "browserify-shim": {
    "jquery": "$",
    "jquery-ui": {
      "depends": "jquery:$"
    },
    "jquery-ui-touch-punch": {
      "depends": "jquery:$"
    },
    "jquery.cookie": {
      "depends": "jquery:$"
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "dependencies": {
    "babel": "^5.1.13",
    "babelify": "^6.0.2",
    "bower": "^1.4.1",
    "browserify": "^9.0.8",
    "browserify-shim": "^3.8.5",
    "debowerify": "^1.2.1",
    "express": "^4.12.3",
    "jade": "^1.9.2",
    "less-css-stream": "^1.0.0",
    "parcelify": "^1.1.1",
    "watchify": "^3.2.0"
  },
  "scripts": {
    "build": "browserify -t babelify -t debowerify es6/main.js -o public/js/bundle.js -p [ parcelify -o public/css/bundle.css -t less-css-stream -d . ]",
    "init": "bower install && cp -rf bower_components/flat-ui/fonts public/",
    "postinstall": "npm run init && npm run build",
    "start": "node app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "watchify -t babelify -t debowerify es6/main.js -o public/js/bundle.js -p [ parcelify -wo public/css/bundle.css -t less-css-stream -d . ] --debug --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yysaki/shikibetsuhyo.git"
  },
  "bugs": {
    "url": "https://github.com/yysaki/shikibetsuhyo/issues"
  },
  "homepage": "https://github.com/yysaki/shikibetsuhyo"
}
