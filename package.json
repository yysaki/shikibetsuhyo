{
  "name": "shikibetsuhyo",
  "version": "0.1.0",
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "jquery-mobile": "./node_modules/jquery-mobile-babel-safe/js/jquery.mobile-1.4.5.js"
  },
  "engines": {
    "node": "0.12.0",
    "npm": "2.5.1"
  },
  "style": [
    "less/style.less",
    "node_modules/jquery-mobile-babel-safe/css/jquery.mobile-1.4.5.min.css"
  ],
  "browserify-shim": {
    "jquery": "$",
    "jquery-ui": {
      "depends": "jquery:$"
    },
    "jquery-ui-touch-punch": {
      "depends": "jquery:$"
    },
    "jquery.cookie": {
      "depends": "jquery:$"
    },
    "jquery-mobile": {
      "depends": "jquery:$"
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.1.2",
    "babel-core": "^6.1.2",
    "babel-preset-es2015": "^6.1.2",
    "babelify": "^7.2.0",
    "bower": "^1.4.1",
    "browserify": "^9.0.8",
    "browserify-shim": "^3.8.5",
    "debowerify": "^1.2.1",
    "express": "^4.12.3",
    "jade": "^1.9.2",
    "jquery": "^2.1.4",
    "jquery-mobile-babel-safe": "^1.4.5-1",
    "less-css-stream": "^1.0.0",
    "parcelify": "^1.1.1",
    "watchify": "^3.2.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "mocha": "^2.3.3",
    "supertest": "^1.1.0"
  },
  "scripts": {
    "build": "babel es6 -d js && browserify -t debowerify js/main.js -o public/js/bundle.js -p [ parcelify -o public/css/bundle.css -t less-css-stream -d . ]",
    "init": "bower install && cp -rf bower_components/flat-ui/fonts public/",
    "postinstall": "npm run init && npm run build",
    "test": "mocha --compilers js:babel-core/register test/**/*.js",
    "start": "node js/app.js",
    "watch": "watchify -t babelify -t debowerify es6/main.js -o public/js/bundle.js -p [ parcelify -wo public/css/bundle.css -t less-css-stream -d . ] --debug --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yysaki/shikibetsuhyo.git"
  },
  "bugs": {
    "url": "https://github.com/yysaki/shikibetsuhyo/issues"
  },
  "homepage": "https://github.com/yysaki/shikibetsuhyo"
}
